{
  "extends": ["next/core-web-vitals", "next/typescript", "plugin:jsx-a11y/recommended", "prettier"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ]
  }
}
